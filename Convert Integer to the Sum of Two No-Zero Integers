class Solution {
public:

    int check(int num){
        int count=0;
        while(num!=0){
            if(num%10==0){
                count++;
            }
            num=num/10;
        }
        return count;
    }
    vector<int> getNoZeroIntegers(int n) {

        vector<int>ans;
        int x=1;
        while(!(check(x)==0) || !((check(n-x))==0)){
            x++;
        }
        ans.push_back(x);
        ans.push_back(n-x);

        return ans;
        
    }
};
